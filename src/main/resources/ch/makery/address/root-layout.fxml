<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="535.0" prefWidth="900.0" stylesheets="@../../../DarkTheme.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.control.RootLayoutController">
  <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#closeFile" text="Close" />
                  <MenuItem mnemonicParsing="false" onAction="#closeFile" text="Reset" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#dark" text="Oscuro" />
                  <MenuItem mnemonicParsing="false" onAction="#white" text="Claro" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#helpWeb" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
  </top>
</BorderPane>
